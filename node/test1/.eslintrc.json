{
  "extends": "strongloop",
  "env": {
      "es6": true,
      "jest": true
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "max-len": "off",
      "quote-props": "off",
      "no-new-func": "off",
      "block-spacing": "off",
      "space-before-function-paren": "off",
      "complexity": ["warn", 3]
  }
}